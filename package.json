{
  "name": "vhs-tape",
  "description": "A test harness for browser elements and components",
  "version": "3.3.0",
  "author": "Bret Comnes <bcomnes@gmail.com> (https://bret.io/)",
  "bugs": {
    "url": "https://github.com/hyperdivision/vhs-tape/issues"
  },
  "bin": "bin.js",
  "dependencies": {
    "browserify": "^16.5.0",
    "events.once": "^2.0.2",
    "fast-on-load": "^1.1.0",
    "glob": "^7.1.4",
    "pump": "^3.0.0",
    "subarg": "^1.0.0",
    "tape": "^4.6.2",
    "tape-run": "bcomnes/tape-run#fix-node-mode"
  },
  "devDependencies": {
    "budo": "^11.6.2",
    "dependency-check": "^3.0.0",
    "hui": "^1.2.5",
    "npm-run-all": "^4.0.0",
    "standard": "^12.0.1"
  },
  "homepage": "https://github.com/hyperdivision/vhs-tape#readme",
  "keywords": [
    "browser testing",
    "frontend",
    "hui",
    "tape",
    "test"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hyperdivision/vhs-tape.git"
  },
  "scripts": {
    "test": "run-s test:*",
    "test:deps": "dependency-check package.json --missing --unused --no-dev -i tape-run",
    "test:example": "./bin.js example.js",
    "test:lint": "standard",
    "start": "budo --live --open example.js"
  }
}
